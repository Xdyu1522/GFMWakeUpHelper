<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="GFMWakeUpHelper.App.App"
             xmlns:local="using:GFMWakeUpHelper.App"
             xmlns:sukiUi="clr-namespace:SukiUI;assembly=SukiUI"
             xmlns:common="clr-namespace:GFMWakeUpHelper.App.Common"
             xmlns:converters="clr-namespace:GFMWakeUpHelper.App.Converters"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->
             
    <Application.Styles>
        <sukiUi:SukiTheme Locale="zh-CN" ThemeColor="Blue"/>
    </Application.Styles>
             
    <Application.Resources>
        <converters:ListStringToVisibilityConverter x:Key="ListStringToVisibilityConverter" />
        <converters:DateTimeToVisibilityConverter x:Key="DateTimeToVisibilityConverter" />
    </Application.Resources>
</Application>